{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": 0,
        "func-names": 0,
    }
}