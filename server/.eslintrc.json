{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": 0,
        "func-names": 0,
        "prettier/prettier": "error"
    }
}